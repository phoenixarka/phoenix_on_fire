        <arguments>
          <argument>
            <kind>1</kind>
            <string>{
    if(display_convs == 1)
    {
        if(story_counter &lt; conv_array_total)
        {
            cur_char = convs_words[story_counter,1];
            next_char = convs_words[story_counter + 1,1];
            cur_pos = convs_words[story_counter,2];
            next_pos = convs_words[story_counter + 1,2];
            if(!audio_is_playing(sfx_window))
            {audio_play_sound(sfx_window,49,false);}
            
            if((cur_char == next_char) &amp;&amp; (cur_pos == next_pos))
            {
                story_counter += 1;
                display_convs = 1;
            }
            else
            {
                close_window = 1;
                display_convs = 0;
            }           
        }
        else
        {instance_create(0,0,obj_fadeout);}
    }
}
</string>
