    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>{
    if(!instance_exists(obj_fadein))
    {
        if(begin_cg1 == 0)
        {
            begin_cg1 = 1;
            open_window = 1;
        }
    
        if(!instance_exists(obj_night)&amp;&amp;!instance_exists(obj_explosionfade))
        {
            if(begin_cg1 == 1)
            {
                //Window open
                if(open_window == 1)
                {
                    if(window_oper_counter &lt; 30)
                    {
                        draw_convs_y1 = convs_y + (((convs_y1 - convs_y)/30) * window_oper_counter);
                        window_oper_counter += 1;
                    }
                    else
                    {
                        open_window = 0;
                        display_convs = 1;
                        window_oper_counter = 0;
                        
                        if(story_counter == warning_begin)
                        {
                            if(!audio_is_playing(sfx_warning))
                            {audio_play_sound(sfx_warning,50,true);}
                        }
                    }
                }
                
                //Window close
                if(close_window == 1)
                {
                    if(window_oper_counter &lt; 30)
                    {
                        draw_convs_y1 = convs_y1 - (((convs_y1 - convs_y)/30) * window_oper_counter);
                        window_oper_counter += 1;
                    }
                    else
                    {
                        //night event        
                        if(story_counter == Explosion_num) 
                        {instance_create(0,0,obj_explosionfade);}
                          
                        if(story_counter == bg2black_num) 
                        {instance_create(0,0,obj_night);}
                        
                        if(story_counter == bg_back_num)
                        {instance_create(0,0,obj_night);}
                        
                        close_window = 0;
                        window_oper_counter = 0;
                        story_counter += 1;
                        open_window = 1;
                    }
                }
                
                if(story_counter == (Explosion_num+1))
                {
                    if(audio_is_playing(sfx_warning))
                    {audio_stop_sound(sfx_warning);}
                }
                
                if(story_counter &gt;= warning_begin &amp;&amp; story_counter &lt;= Explosion_num)
                {
                    if(alert_status == 0)
                    {
                        if(alert_counter &gt;= 0.8)
                        {alert_status = 1;}
                        else
                        {alert_counter += 0.02;}
                    }
                    else if(alert_status == 1)
                    {
                        if(alert_counter &lt;= 0)
                        {alert_status = 0;}
                        else
                        {alert_counter -= 0.02;}
                    }
                }
                else
                {
                    alert_status = 0;
                    alert_counter = 0;
                }
                
                //left and right convs
                if(convs_words[story_counter,2] == 1)
                {draw_char_x = char_x;}
                else
                {draw_char_x = char_x1;}
            }
        }
    }
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
