            <kind>1</kind>
            <string>{
    if(warningactive == 1)
    {
        counter += 1;
        if(warning_sound == 0)
        {warning_sound = 1;}
        
        if(warning_sound == 1)
        {
            audio_play_sound(sfx_warning,50,false);
            warning_sound = 2;
        }
        
        
        if(fade == 0)
        {
            if(img_counter &lt;= 14)
            {
                if(counter == 2)
                {
                    image_index = img_counter;
                    img_counter += 1;
                    counter = 0;
                }
                
                if(img_counter == 13)
                {
                    counter = 0;
                    img_counter += 1;
                    image_index = img_counter;
                    fade = 1;
                }
            }
        }  
        
        if(fade == 1)  
        {    
            if(warning_round &lt; 4)
            {
                if(counter == 10)
                {
                    image_index = 14;
                }
                        
                if(counter == 20)
                {
                    image_index = 15;
                    counter = 0;
                    warning_round += 1;
                }            
            }
            else
            {
                fade = 2;
                counter = 0;
                img_counter = 15;
            }
        }
     
        if(fade == 2)
        {
            if(img_counter &gt; 0)
            {
                if(counter == 2)
                {
                    image_index = img_counter;
                    img_counter -= 1;
                    counter = 0;
                }
            }
            else
            {
                if(room != rm_tutorial)
                {
                    if(bgm_laststage == 0)
                    {
                        audio_stop_sound(bgm_lv1);
                        audio_play_sound(bgm_lv1_boss,50,true);
                    }
                    
                    if(room == rm_level5)
                    {
                         audio_stop_sound(bgm_lv5);
                        //should be lv5 boss
                        audio_play_sound(bgm_lv1_boss,50,true);
                    }
                }
                instance_destroy();
            }
        }
    }
}
