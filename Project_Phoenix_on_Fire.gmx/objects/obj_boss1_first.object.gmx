    {
        MC_counter += 1;    
        
        if(MC_counter == 1)
        {image_index  = 3;}
         
        if(MC_counter == 5)
        {image_index  = 4;}    
        
        if(MC_shoot_counter &lt; 8)
        {
            if(MC_counter == 30)
            {
                rep_counter = 0;
                if(instance_exists(obj_fighterjet_parent))
                {
                    player_Xpos = obj_fighterjet_parent.x;
                    player_Ypos = obj_fighterjet_parent.y
                }
                else
                {
                    player_Xpos = 0;
                    player_Ypos = y + 10;
                }
                repeat(4)
