        }
        
        if(MC_counter &gt;= 30)
        {
            main_cannon = 2;
            MC_counter = 0;
            MC_shoot_counter = 0;
        }   
    }
    
    if(main_cannon == 2)
    {
        MC_counter += 1;
    
        if(MC_counter == 1)
        {image_index  = 3;}
         
        if(MC_counter == 5)
        {
            image_index  = 0;
            MC_counter = 0;
            main_cannon = 0;
        }    
    }
}

