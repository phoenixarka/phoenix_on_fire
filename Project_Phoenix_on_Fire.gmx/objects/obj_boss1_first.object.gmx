                    {x_offen = 68;}
                    
                    if(rep_counter &lt; 2)
                    {y_offen = 67;}
                    else
                    {y_offen = 82;}
                    
                    DeltaX = (x + x_offen) - player_Xpos;
                    DeltaY = (y + y_offen) - player_Ypos;
                    
                    if(DeltaY != 0)
                    {MG_dir_offen = (arctan(DeltaX/DeltaY) / pi) * 180;}
