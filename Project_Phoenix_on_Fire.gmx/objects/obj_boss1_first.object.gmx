    TG_bullet_speed = 10;
    x_offen = 0; 
    dir_offen = 0;
    
    if(tail_gun == 1)
    {
        TG_counter += 1;
    
        if(TG_counter == 1)
        {image_index  = 1;}
         
        if(TG_counter == 5)
        {
            image_index  = 2;
        }    
        
        if(TG_shoot_counter &lt; 8)
        {
            if(TG_shoot_counter &lt; 4)
            {i = TG_shoot_counter;}
            else
            {i = TG_shoot_counter mod 8;}
            
            iremain = (i+1) mod 4;
            
            if(i &gt; 3)
            {dir_offen = 25;}
            
            if(iremain == 2 || iremain ==3)
            {
                x_offen = 196;
                if((iremain mod 2) != 0)
                {x_offen *= -1;}
            }
            else
            {
                x_offen = 146;
                if((iremain mod 2) != 0)
                {x_offen *= -1;}
            }
            
            if(TG_counter == 25)
            {
                TG_bullet_dir = 0;        
                repeat(8)
                {
                    TG_bullet = instance_create(x + x_offen,y - 53,obj_bullet_enemy_adv);
                    TG_bullet.direction = TG_bullet_dir + dir_offen;
                    TG_bullet.speed = TG_bullet_speed;
                    TG_bullet_dir += 45;                    
                }  
                TG_shoot_counter += 1; 
                TG_counter = 15;             
            }
        }
        
        if(TG_counter &gt;= 30)
        {
            tail_gun = 2;
            TG_counter = 0;
            TG_shoot_counter = 0;   
        }   
    }
    
    if(tail_gun == 2)
    {
        TG_counter += 1;
    
        if(TG_counter == 1)
        {image_index  = 1;}
         
        if(TG_counter == 5)
        {
            image_index  = 0;
            TG_counter = 0
            tail_gun = 0;
        }    
    }
    
}

