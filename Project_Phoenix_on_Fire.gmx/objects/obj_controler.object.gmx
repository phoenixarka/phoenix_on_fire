            <string>{
    if(level_pass == 1)
    {         
        if(clearSounds_win == 0)
        {
            audio_stop_all();
            clearSounds_win = 1;
        }
        
        if(win_sound == 0)
        {
            if(audio_is_playing(sfx_level_clear))
            {audio_stop_sound(sfx_level_clear);}
            audio_play_sound(sfx_level_clear,51,false);
            win_sound = 1;
        }      
        
        //jet postion adjust
        if(instance_exists(obj_fighterjet_parent))
        {        
            //create value pass instance
            if(!instance_exists(obj_level_up_passV))
            {
                passvalue = instance_create(x,y,obj_level_up_passV);
                passvalue.next_rm_Num = levelpass_Num + 1;
                passvalue.jet_weapon_lv = obj_fighterjet_parent.weapon_lv;
            }
            
            obj_fighterjet_parent.can_control = 0;
            obj_fighterjet_parent.canfire = 0;
            obj_fighterjet_parent.ini = 0;
            
            //the position that fighterjet should be
            endPointX = view_xview + (view_wview /2);
            endPointY = view_yview + (view_hview*0.9);
            
            if(obj_fighterjet_parent.x != endPointX || obj_fighterjet_parent.y != endPointY &amp;&amp; fly_pass_level == 0)
            {
                jet_acc = 5;
                //Move obj_fighterjet_parent to end point
                //X
                if(obj_fighterjet_parent.x &lt; endPointX)
                {
                    if(endPointX - obj_fighterjet_parent.x &lt; 5)
                    {obj_fighterjet_parent.x = endPointX;}
                    else
                    {obj_fighterjet_parent.x += 5;}
                }
                else if(obj_fighterjet_parent.x &gt; endPointX)
                {
                    if(obj_fighterjet_parent.x - endPointX &lt; 5)
                    {obj_fighterjet_parent.x = endPointX;}
                    else
                    {obj_fighterjet_parent.x -= 5;}
                }
                
                //Y
               if(obj_fighterjet_parent.y &lt; endPointY)
                {
                    if(endPointY - obj_fighterjet_parent.y &lt; 5)
                    {obj_fighterjet_parent.y = endPointY;}
                    else
                    {obj_fighterjet_parent.y += 5;} 
                }
                else if(obj_fighterjet_parent.y &gt; endPointY)
                {
                    if(obj_fighterjet_parent.y - endPointY &lt; 5)
                    {obj_fighterjet_parent.y = endPointY;}
                    else
                    {obj_fighterjet_parent.y -= 5;}
                }
                
                if((abs(obj_fighterjet_parent.x - endPointX) &lt; 1) &amp;&amp; (abs(obj_fighterjet_parent.y - endPointY) &lt; 1))
                {fly_pass_level = 1;}
            }
            else
            {
                if(!audio_is_playing(sfx_level_clear))
                {
                    if(obj_fighterjet_parent.y &gt; view_yview - 50)
                    {
                        obj_fighterjet_parent.y -= jet_acc;
                        jet_acc += 5;
                    }
                    else 
                    {
                        if(!instance_exists(obj_level_result))
                        {
                            instance_create(0,0,obj_level_result);
                            level_pass = 0;
                            clearSounds_win = 0;
                            win_sound = 0;
                            jet_acc = 0;
                            fly_pass_level = 0;
                        }
                    }
                }
            }
        }
    }
}  
