            <string>{
    if(tankcannon == 1)
    {
        if(TC_fire == 0)
        {
            if(TC_delay &gt; 0)
            {TC_delay -= 1;}
            else
            {TC_fire = 1;}
        }
        
        if(TC_fire == 1)
        {
            TC_offset = 0;
            while(y+295 + TC_offset &lt; view_yview+view_hview)
            {
                if((TC_counter mod 2) == 0)
                {
                    instance_create(x-13,y+295 + TC_offset,obj_boss_machinegun);
                    instance_create(x-23,y+295 + TC_offset,obj_boss_machinegun);
                }
                else
                {
                    instance_create(x+13,y+295 + TC_offset,obj_boss_machinegun);
                    instance_create(x+23,y+295 + TC_offset,obj_boss_machinegun);
                }
                TC_offset += 5;
            }
            TC_fire = 2;
        }
        
        if(TC_fire == 3)
        {
            if(TC_intv_counter &gt;= 10)
            {
                TC_fire = 1;
                TC_intv_counter = 0;
            }
            TC_intv_counter += 1;
        }   
         
         
         if(TC_counter &gt;= 8)
         {
            tankcannon = 0;
            TC_counter = 0;
            TC_intv_counter = 0;
            TC_fire = 0;
            TC_delay = 45;
         }        
    }
}
