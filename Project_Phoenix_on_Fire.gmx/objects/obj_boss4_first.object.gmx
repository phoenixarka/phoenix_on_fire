            <string>{
    if(rocketgun == 1)
    {        
        if(RG_counter == 1 || RG_counter == 3)
        {
            RG_intv_counter += 1;
            
            if(RG_intv_counter &gt;= 30)
            {
                RG_counter += 1;
                RG_intv_counter = 0;
            }
        }
    
        if(RG_counter == 0 || RG_counter == 4)
        {
            //left
            for(i = 0; i &lt; 4; i++)
            {
                effect_create_above(ef_smoke,x-150 + (i*20),y+50,1,c_black);
                effect_create_above(ef_smoke,x-150 + (i*20),y+50,0,c_red);
                effect_create_above(ef_smoke,x-150 + (i*20),y+50,0,c_yellow);
            }
            
            for(i = 0; i &lt; 3; i++)
            {
                bullet1 = instance_create(x-120,y+120,obj_bullet_enemy_adv);
                bullet1.direction = -120;
                bullet1.speed = 8 + (i*2);
                
                bullet2 = instance_create(x-120,y+120,obj_bullet_enemy_adv);
                bullet2.direction = -60;
                bullet2.speed = 8 + (i*2);
                
                bullet3 = instance_create(x-120,y+120,obj_bullet_enemy_adv);
                bullet3.direction = -105;
                bullet3.speed = 8 + (i*2);
                
                bullet4 = instance_create(x-120,y+120,obj_bullet_enemy_adv);
                bullet4.direction = -75;
                bullet4.speed = 8 + (i*2);
            }
            
            for(i = 0; i &lt; 3; i++)
            {
                for(j = 0; j &lt; 3; j++)
                {
                    bullet1 = instance_create(x-140 + (i*20),y+50,obj_bullet_enemy_adv);
                    bullet1.direction = -90;
                    bullet1.speed = 8 +(j*2);
                }
            }      
            RG_counter += 1;
        }
        
        if(RG_counter == 2 || RG_counter == 5)
        {
            //right
            for(i = 0; i &lt; 4; i++)
            {
