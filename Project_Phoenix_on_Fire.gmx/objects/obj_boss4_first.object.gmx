    if(rocketfall== 1 || rocketfall== 2)
    {
        predict_color = c_green;
        
        if(RF_delay &lt; 60 &amp;&amp; RF_delay &gt;= 30)
        {predict_color = c_yellow;}
        if(RF_delay &lt; 30)
        {predict_color = c_red;}
                
