            draw_set_alpha(1.0);
        }
        
        if(TC_fire == 2)
        {
            TC_offset = 0;
            while(y+295 + TC_offset &lt; view_yview+view_hview)
            {
                if((TC_counter mod 2) == 0)
                {
                    effect_create_below(ef_flare,x-18,y+295 + TC_offset,1,c_red);
                    effect_create_above(ef_flare,x-18,y+295 + TC_offset,0,c_yellow);
