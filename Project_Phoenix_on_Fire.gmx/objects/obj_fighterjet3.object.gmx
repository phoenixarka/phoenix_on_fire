            <string>{
    if(can_control == 1)
    {
        //direction control
        if(obj_pad_stick.DirmouseNum != -1)
        {            
            //control X
            if(x &gt;= view_xview + (sprite_width/2) &amp;&amp; obj_pad_stick.DirdeltaX &gt; 0)
            {x += obj_pad_stick.DirdeltaX;}
            else if(x &lt;= view_xview + view_wview - (sprite_width/2) &amp;&amp; obj_pad_stick.DirdeltaX &lt; 0)
            {x += obj_pad_stick.DirdeltaX;}
            
            //border X
            if(x &lt; view_xview + (sprite_width/2))
            {x = view_xview + (sprite_width/2);}
            else if (x &gt; (view_xview + view_wview - (sprite_width/2)))
            {x = view_xview + view_wview - (sprite_width/2);}
            
            //control Y
            if(y &gt;= view_yview + (sprite_height/2) &amp;&amp; obj_pad_stick.DirdeltaY &gt; 0)
            {y += obj_pad_stick.DirdeltaY;}
            if(y &lt;= view_yview + view_hview - (sprite_height/2) &amp;&amp; obj_pad_stick.DirdeltaY &lt; 0)
            {y += obj_pad_stick.DirdeltaY;}
            
            //border Y
            if(y &lt; view_yview + (sprite_height/2))
            {y = view_yview + (sprite_height/2);}
            else if (y &gt; view_yview + view_hview - (sprite_height/2))
            {y = view_yview + view_hview - (sprite_height/2);}
        }
        
        //bomb control
        if((obj_pad_stick.bomb_press == 1) &amp;&amp; (obj_bombs.bomb &gt;= 1) &amp;&amp; (bomb_avalible == 1))
        {
            obj_pad_stick.bomb_press += 1;
            obj_bombs.bomb -= 1;
            bomb_avalible = 0;
            sound_play(sfx_Bomb_Laser_rdy);
            instance_create(x -172, y - 51, obj_B_Laser_rdy);
            alarm[4] = 10;
        }
        
        //charge control
        //press
        if((obj_pad_stick.charge_press == 1) &amp;&amp; (weapon_lv &gt; 1) &amp;&amp; (can_charge == 1))
        {
            canfire = 0;
            if(health &lt; 100)
            {health += 3.5;}
        }        
        
        //release
        if((obj_pad_stick.charge_press == 0))
        {
            if(health &gt;= 100)
            {
                obj_energy.charge_release = 1;
                can_charge = 0;
            }
            else
            {
                if(obj_energy.charge_release != 1)
                {canfire = 1;}
            }
        }        
    }
}
</string>
