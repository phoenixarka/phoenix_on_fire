          <argument>
            <kind>1</kind>
            <string>{
    //main_cannon == 0 keep current
    //main_cannon == 1 open main cannon
    //main_cannon == 2 close main cannon
    
    MG_bullet_dir = -90;
    MG_bullet_speed = 12;
    x_offen = 0;
    y_offen = 0;
    
    if(main_cannon == 1)
    {
        MC_counter += 1;    
        
        if(MC_counter == 1)
        {image_index  = 3;}
         
        if(MC_counter == 5)
        {image_index  = 4;}    
        
        if(MC_shoot_counter &lt; 8)
        {
            if(MC_counter == 30)
            {
                rep_counter = 0;
                if(instance_exists(obj_fighterjet_parent))
                {
                    player_Xpos = obj_fighterjet_parent.x;
                    player_Ypos = obj_fighterjet_parent.y
                }
                else
                {
                    player_Xpos = 0;
                    player_Ypos = y + 10;
                }
                repeat(4)
                {                   
                    if(rep_counter == 0)
                    {x_offen = 17;}
                    else if(rep_counter == 1)
                    {x_offen = 17;}
                    else if(rep_counter == 2)
                    {x_offen = -17;}
                    else if(rep_counter == 3)
                    {x_offen = -17;}
                    
                    if(rep_counter mod 2  == 1)
                    {y_offen = 4;}
                    else
                    {y_offen =-16;}
                    
                    DeltaX = (x + x_offen) - player_Xpos;
                    DeltaY = (y + y_offen) - player_Ypos;
                    
                    if(DeltaY != 0)
                    {MG_dir_offen = (arctan(DeltaX/DeltaY) / pi) * 180;}
                    else
                    {MG_dir_offen = 0;}
                    
                    MG_bullet = instance_create(x + x_offen,y + y_offen,obj_bullet_enemy_grv);
                    MG_bullet.direction = MG_bullet_dir + MG_dir_offen;
                    MG_bullet.speed = MG_bullet_speed; 
                    rep_counter += 1;                  
                }  
                MC_shoot_counter += 1; 
                MC_counter = 5;             
            }            
        }
        
        if(MC_counter &gt;= 30)
        {
            main_cannon = 2;
            MC_counter = 0;
            MC_shoot_counter = 0;
        }   
    }
    
    if(main_cannon == 2)
    {
        MC_counter += 1;
    
        if(MC_counter == 1)
        {image_index  = 3;}
         
        if(MC_counter == 5)
        {
            image_index  = 0;
            MC_counter = 0;
            main_cannon = 0;
        }    
    }
}

