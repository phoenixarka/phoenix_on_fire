        <arguments>
          <argument>
            <kind>1</kind>
            <string>{
    // 1 left shoot, 2 left delay; 3 right shoot; 4 right delay
    //left
    if(sub1_rocket == 1)
    {
        if(S1R_counter == 0)
        {y_offset = 0;}
        if(S1R_counter == 1)
        {y_offset = 39;}
        if(S1R_counter == 2)
        {y_offset = 20;}
        if(S1R_counter == 3)
        {y_offset = 0;}
        if(S1R_counter == 4)
        {y_offset = 39;}
        if(S1R_counter == 5)
        {y_offset = 20;}
        if(S1R_counter mod 2 = 1)
        {x_offset = 163;}
        else
        {x_offset = 149;}
        
        if(S1R_intv_counter &gt;=2)
        {
            effect_create_above(ef_smoke,x-x_offset,y-y_offset,0,c_black);
            effect_create_above(ef_smoke,x-x_offset,y-y_offset,0,c_red);
            effect_create_above(ef_smoke,x-x_offset,y-y_offset,0,c_yellow);
            S1R_intv_counter = 0;
            S1R_counter += 1;
            
            if(S1R_counter &gt; 5)
            {sub1_rocket = 2;} 
        }
        S1R_intv_counter += 1;
    }
    
    if(sub1_rocket == 2)
    {
        S1R_delay -= 1;
        if(S1R_delay &lt; 0)
        {   
            instance_create(x - 95,y + 500,obj_boss_rocket);
            instance_create(x - 275,y + 250,obj_boss_rocket);
            instance_create(x - 185,y + 500,obj_boss_rocket);
            instance_create(x - 45,y + 250,obj_boss_rocket);
            instance_create(x - 225,y + 500,obj_boss_rocket);
            instance_create(x - 135,y + 150,obj_boss_rocket);
            sub1_rocket = 3;
            S1R_counter = 0;
            S1R_delay = 120;
            S1R_intv_counter = 0;
        }
    }
    
    //right
    if(sub1_rocket == 3)
    {
        if(S1R_counter == 0)
        {y_offset = 0;}
        if(S1R_counter == 1)
        {y_offset = 39;}
        if(S1R_counter == 2)
        {y_offset = 20;}
        if(S1R_counter == 3)
        {y_offset = 0;}
        if(S1R_counter == 4)
        {y_offset = 39;}
        if(S1R_counter == 5)
        {y_offset = 20;}
        if(S1R_counter mod 2 = 1)
        {x_offset = 163;}
        else
        {x_offset = 149;}
        
        if(S1R_intv_counter &gt;=2)
        {
            effect_create_above(ef_smoke,x+x_offset,y-y_offset,0,c_black);
            effect_create_above(ef_smoke,x+x_offset,y-y_offset,0,c_red);
            effect_create_above(ef_smoke,x+x_offset,y-y_offset,0,c_yellow);
            S1R_intv_counter = 0;
            S1R_counter += 1;
            
            if(S1R_counter &gt; 5)
