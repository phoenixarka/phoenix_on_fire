        <arguments>
          <argument>
            <kind>1</kind>
            <string>{   
    if(begin_cg1)
    {
        if(!instance_exists(obj_night))
        {
            frame_color = $1B1B1B;
            frame_fillcolor = $FFFFFF;
            
            if(convs_words[story_counter,2] == 1)
            {
                //draw character frame
                draw_roundrect_color(draw_char_x,char_y,draw_char_x+96,char_y+96,0,frame_color,true);
                for(i = 0; i &lt; 5; i++)
                {draw_roundrect_color(draw_char_x - i,char_y - i,draw_char_x + 96 + i,char_y + 96 + i,0,frame_color,true);}
                
                draw_set_alpha(0.5);
                draw_roundrect_color(draw_char_x,char_y,draw_char_x+96,char_y+96,frame_fillcolor,frame_fillcolor,false);
                draw_set_alpha(1);  
            }
            else
            {
                //draw character frame
                draw_roundrect_color(char_x1,char_y,char_x1+96,char_y+96,0,frame_color,true);
                for(i = 0; i &lt; 5; i++)
                {draw_roundrect_color(char_x1 - i,char_y - i,char_x1 + 96 + i,char_y + 96 + i,0,frame_color,true);}
                
                draw_set_alpha(0.5);
                draw_roundrect_color(char_x1,char_y,char_x1+96,char_y+96,frame_fillcolor,frame_fillcolor,false);
                draw_set_alpha(1);
            }
            
            //draw conversation frame
            draw_roundrect_color(convs_x,convs_y,convs_x1,draw_convs_y1,0,frame_color,true);
            for(i = 0; i &lt; 5; i++)
            {draw_roundrect_color(convs_x - i,convs_y - i,convs_x1 + i,draw_convs_y1 + i,0,frame_color,true);}
            draw_set_alpha(0.5);
            draw_roundrect_color(convs_x,convs_y,convs_x1,draw_convs_y1,frame_fillcolor,frame_fillcolor,false);
            draw_set_alpha(1);
        }
    }
}
