            <kind>1</kind>
            <string>{
    P1x = view_xview + (view_wview*0.1); P1y = view_yview + (view_hview*0.1);    // P1     P2
    P2x = view_xview + (view_wview*0.9); P2y = view_yview + (view_hview*0.1);    //
    P3x = view_xview + (view_wview*0.9); P3y = view_yview + (view_hview*0.9);    //
    P4x = view_xview + (view_wview*0.1); P4y = view_yview + (view_hview*0.9);    // P4     P3
    Lwidth = 10;    
    
    //The Frame
    draw_line_width_color(P1x,P1y,P2x + (Lwidth/2),P2y,Lwidth,255,0);
    draw_line_width_color(P2x,P2y,P3x,P3y + (Lwidth/2),Lwidth,0,255);
    draw_line_width_color(P3x,P3y,P4x - (Lwidth/2),P4y,Lwidth,255,0);
    draw_line_width_color(P4x,P4y,P1x,P1y - (Lwidth/2),Lwidth,0,255);   
    
    //The inner background
    
    RectX1 = P1x + (Lwidth/2);
    RectY1 = (P1y+P4y)/2;
    RectX2 = P2x - (Lwidth/2);
    RectY2 = (P2y+P3y)/2;
    Rect_width = (P4y-P1y) - Lwidth;   
     
    draw_set_alpha(0.5);
    draw_line_width_color(RectX1,RectY1,RectX2,RectY2,Rect_width,0,0); 
    draw_set_alpha(1);
}
