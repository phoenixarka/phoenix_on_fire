        {image_index = spr_fightjet_2;}    
        if(obj_controler.currentJet == 2)
        {image_index = spr_fightjet_3;} 
        
        life_sprite_Xoffset = (sprite_get_width(image_index)) * 1.3;
        life_sprite_Yoffset = (sprite_get_height(image_index)) /3;
        
        draw_sprite_ext(image_index, 0, draw_lifeX + (sprite_get_width(image_index)/2), draw_lifeY + (sprite_get_height(image_index)/2), 1, 1, 0, c_white, 1 );
        draw_text_color(draw_lifeX + life_sprite_Xoffset,draw_lifeY + life_sprite_Yoffset,"X", $FFFFFF, $FFFFFF, $FFFFFF, $FFFFFF, 1);
        draw_text_color(draw_lifeX + (life_sprite_Xoffset * 1.7), draw_lifeY + life_sprite_Yoffset, lives_count, $FFFFFF, $FFFFFF, $FFFFFF, $FFFFFF, 1);
        draw_text_color (draw_lifeX +(life_sprite_Xoffset * 2.5), draw_lifeY + life_sprite_Yoffset, "=", $FFFFFF, $FFFFFF, $FFFFFF, $FFFFFF, 1);
        draw_text_color (draw_lifeX +(life_sprite_Xoffset * 3), draw_lifeY + life_sprite_Yoffset, lives_bounes_score , $FFFFFF, $FFFFFF, $FFFFFF, $FFFFFF, 1);
    }
    
    if(show_bomb_bounes == 1)
    {
        //Draw ( [bomb_pic] X bombs = score_bounes)
        draw_bombX = view_xview + (view_wview * 0.1) + (allwidth * 0.1);
        draw_bombY = view_yview + (view_hview * 0.1) + (allheight * 0.32);
          
        bomb_sprite_Xoffset = (sprite_get_width(spr_bomb)*2) * 1.3;
        bomb_sprite_Yoffset = (sprite_get_height(spr_bomb)*2) /3;
        
        draw_sprite_ext( spr_bomb, 0, draw_bombX, draw_bombY, 2, 2, 0, c_white, 1 );
        draw_text_color (draw_bombX + bomb_sprite_Xoffset,draw_bombY + bomb_sprite_Yoffset,"X", $FFFFFF, $FFFFFF, $FFFFFF, $FFFFFF, 1);
        draw_text_color (draw_bombX + (bomb_sprite_Xoffset * 1.7), draw_bombY + bomb_sprite_Yoffset, bomb_count, $FFFFFF, $FFFFFF, $FFFFFF, $FFFFFF, 1);
        draw_text_color (draw_bombX +(bomb_sprite_Xoffset * 2.5), draw_bombY + bomb_sprite_Yoffset, "=", $FFFFFF, $FFFFFF, $FFFFFF, $FFFFFF, 1);
        draw_text_color (draw_bombX +(bomb_sprite_Xoffset * 3), draw_bombY + bomb_sprite_Yoffset, bomb_bounes_score, $FFFFFF, $FFFFFF, $FFFFFF, $FFFFFF, 1);
    }
    
    if(show_score_all == 1)
    {
        //Draw Total score
        draw_totalX = view_xview + (view_wview * 0.1) + (allwidth * 0.1);
        draw_totalY = view_yview + (view_hview * 0.1) + (allheight * 0.5);
        
        total_offset = allwidth * 0.45;
        
        draw_text_color (draw_totalX,draw_totalY,"Total Score:", $FFFFFF, $FFFFFF, $FFFFFF, $FFFFFF, 1);
        draw_text_color (draw_totalX + total_offset ,draw_totalY,all_score, $FFFFFF, $FFFFFF, $FFFFFF, $FFFFFF, 1);
    }    
    
    if(show_rateStr == 1)
    {
        //Draw Rate
        draw_rateX = view_xview + (view_wview * 0.1) + (allwidth * 0.6);
        draw_rateY = view_yview + (view_hview * 0.1) + (allheight * 0.7);  
        
        draw_rateStrX = view_xview + (view_wview * 0.1) + (allwidth * 0.05);
        draw_rateStrY = view_yview + (view_hview * 0.1) + (allheight * 0.8);  
        
        Rate_scale_offset = 50;        
        
        //Rate Calculate
        
        if(all_score &lt; 20065)
        {rate_level = 5;}
        if(all_score &gt;= 20065)
        {rate_level = 4;}
        if(all_score &gt;= 22515)
        {rate_level = 3;}     
        if(all_score &gt;= 23985)
        {rate_level = 2;}
        if(all_score &gt;= 24965)
        {rate_level = 1;}
        if(all_score &gt;= 26315)
        {rate_level = 0;}
        
        if(rate_level == 0)
        {Rate_levelStr = "S";}
        if(rate_level == 1)
        {Rate_levelStr = "A";}
        if(rate_level == 2)
        {Rate_levelStr = "B";}
        if(rate_level == 3)
        {Rate_levelStr = "C";}
        if(rate_level == 4)
        {Rate_levelStr = "D";}
        if(rate_level == 5)
        {Rate_levelStr = "P";}
        
        draw_set_font(font_rateStr);
        draw_text_ext_transformed_color(draw_rateStrX, draw_rateStrY, "Rate Class", 0, allwidth, 1, 1, 30, $0DC6FC, $FFFFFF, $0DC6FC, $FFFFFF, 1);
    }
    draw_set_font(font_rate);
    if(show_rate == 1)
    {draw_text_ext_transformed_color(draw_rateX - (Rate_scale_offset * (rate_scale -1)), draw_rateY - (Rate_scale_offset * (rate_scale -1)),
    Rate_levelStr, 0, allwidth, rate_scale, rate_scale, 0, 255, $0DC6FC, 255, $0DC6FC, 1);}
    
    draw_set_font(font_default);
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
