    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>{
    if(CounterAll &lt; 120)
    {CounterAll += 1;}
    
    if(CounterAll == 30)
    {show_title = 1;}
    
    if(CounterAll == 60)
    {show_life_bounes = 1;}
    
    if(CounterAll == 90)
    {show_bomb_bounes = 1;}
    
    if(CounterAll &gt;= 120 &amp;&amp; CounterAll &lt;= 150)
    {
        show_score_all = 1;
        if(CounterAll &lt; 150)
        {CounterAll += 1;}
        
        //set score offset
        offset_live_bounes = 100;
        offset_bomb_bounes = 100;
        
        if(CounterAll == 150)
        {
            if(lives_bounes_score &gt; 0)
            {
                lives_bounes_score -= offset_live_bounes;
                all_score += offset_live_bounes;
                score += offset_live_bounes;
                audio_play_sound(sfx_score_count,55,false);
            }
            if(lives_bounes_score == 0 &amp;&amp; bomb_bounes_score &gt; 0)
            {
                bomb_bounes_score -= offset_bomb_bounes;
                all_score += offset_bomb_bounes;
                score += offset_bomb_bounes;
                audio_play_sound(sfx_score_count,55,false);
            }
            
            if(lives_bounes_score == 0 &amp;&amp; bomb_bounes_score == 0)
            {CounterAll += 1;}
        }
    }
    
    if(CounterAll &gt;= 151 &amp;&amp; CounterAll &lt; 180)
    {
        show_rateStr = 1;
        CounterAll += 1;
    }
    
    if(CounterAll == 180)
    {
        if(!audio_is_playing(sfx_rate_fall))
        {audio_play_sound(sfx_rate_fall,55,false);}
        show_rate = 1;
        CounterAll += 1;
    }
    
    //Rate Scale
    if(rate_scale &gt; 1 &amp;&amp; show_rate == 1)
    {rate_scale -= 1;}

    //If finish
    if(rate_scale == 1)
    {
        if(rate_done == 0)
        {
            if(audio_is_playing(sfx_rate_fall))
            {
                audio_stop_sound(sfx_rate_fall);
                audio_play_sound(sfx_rate_done,55,false);
            }
            rate_done = 1;
        }
        if(CounterAll &lt; 225)
        {CounterAll += 1;}
    }
    
    if(CounterAll == 225)
    {result_finish = 1;}
    
    if(result_finish == 1 &amp;&amp; CounterAll &lt; 300)
    {CounterAll += 1;}
    
    if(CounterAll &gt;= 300)
    {
        instance_create(x,y,obj_fadeout);
        if(canjumproom == 1)
        {
            if(level_Num == 1)
            {room_goto(room_CG1);}
            if(level_Num == 2)
            {room_goto(room_CG2);}
            if(level_Num == 3)
            {room_goto(room_CG3);}
            if(level_Num == 4)
            {room_goto(room_CG4);}
            if(level_Num == 5)
            {room_goto(room_CG5);}
        }
    }
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
